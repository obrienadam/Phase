; Test case for the 2D sudden expansion

CaseName SuddenExpansion2D

Solver
{
	timeDependent Off
	maxIterations 100
	timeStep 0.1
	maxTime 10
	maxCo 0.8

	numInnerIterations 1
	numPressureCorrections 1
	momentumRelaxation 1
	pressureCorrectionRelaxation 0.7
}

LinearAlgebra
{
	uEqn
	{
		lib trilinos
		solver BiCGSTAB
		preconditioner DIAGONAL
		iluFill 0
		maxPreconditionerUses 10
		tolerance 1e-3
	}

	pEqn
	{
		lib trilinos
		solver BiCGSTAB
		preconditioner RILUK
		iluFill 3
		maxPreconditionerUses 10
	}

	pCorrEqn
	{
		lib trilinos
		iluFill 2
	}
}

Properties
{
	rho 1
	mu 1.e-2
	g (0,0)
	gamma 0.001
}

Grid
{
	type cgns
	filename "case/SuddenExpansionMesh.cgns"
}

Viewer
{
  type tecplot
  vectorFields u,gradP,gradPhi
  scalarFields p
}

System
{
  fileWriteFrequency 10
}
