; Test case for the 2D immersed boundary cylinder

CaseName ImmersedBoundaryCylinder2D

Solver
{
        type "fractional step direct-forcing"
	timeStep 2.5e-3
	maxCo 1
	maxTime 200
	smoothingKernelRadius 0
}

LinearAlgebra
{
	uEqn
	{
		lib belos
		solver TFQMR
		preconditioner DIAGONAL
	}

	pEqn
	{
		lib eigen
	}

	fbEqn
	{
		lib belos
		solver gmres
		preconditioner DIAGONAL
	}

        extEqn
	{
		lib belos
		solver gmres
		preconditioner diagonal
	}
}

Properties
{
	rho 10
	mu 0.0001 ; Re = 100
	g (0,0)
	sigma 0
}

Grid
{
	type rectilinear
	nCellsX 400
	nCellsY 200

	; Rectilinear parameters
	width 10
	height 5
	minBufferWidth 0.05
	origin (0.5,0)

	refineX (1,3)
	refineY (1.5,3.5)
}

Viewer
{
	vectorFields u,us,fb,gradP,gradp
	scalarFields p,divU
	integerFields cellStatus
}
